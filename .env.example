# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# EmailJS Configuration (for sending confirmation emails)
VITE_EMAILJS_SERVICE_ID=your_emailjs_service_id
VITE_EMAILJS_TEMPLATE_ID=your_emailjs_template_id
VITE_EMAILJS_ADMIN_TEMPLATE_ID=your_emailjs_admin_template_id
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key

# EmailJS Status Update Templates (for admin status notifications)
VITE_EMAILJS_APPROVED_TEMPLATE_ID=template_approved_id
VITE_EMAILJS_REJECTED_TEMPLATE_ID=template_rejected_id
VITE_EMAILJS_UNDER_REVIEW_TEMPLATE_ID=template_under_review_id
VITE_EMAILJS_NEED_DOCS_TEMPLATE_ID=template_need_docs_id

# Resend API for automated email sending
RESEND_API_KEY=your_resend_api_key_here

# Example:
# VITE_SUPABASE_URL=https://your-project.supabase.co
# VITE_SUPABASE_ANON_KEY=your-anon-key-here
# VITE_EMAILJS_SERVICE_ID=service_xxxxxxx
# VITE_EMAILJS_TEMPLATE_ID=template_xxxxxxx
# VITE_EMAILJS_PUBLIC_KEY=xxxxxxxxxxxxxxx